      "variables": [
        {
          "name": "CMAKE_CXX_FLAGS_RELEASE",
          "value": "/MT -g -O2 -DNDEBUG -DUSE_BMI2 -march=native -flto",
          "type": "STRING"
        },
        {
          "name": "CMAKE_EXE_LINKER_FLAGS_RELEASE",
          "value": "/INCREMENTAL:NO",
          "type": "STRING"
        }
      ]
    }
  ]
